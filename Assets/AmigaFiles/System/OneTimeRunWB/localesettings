COPY >NIL: "SYS:WBStartup/Disabled/#?" TO "SYS:WBStartup/" ALL FORCE CLONE
DELETE >NIL: "SYS:WBStartup/Disabled" ALL FORCE
ECHO "Choose your regional settings then press the save button. Once you do this your Amiga will reboot"
ECHO ""
IF $KickstartVersion EQ 3.9
   C:AddDataTypes REFRESH QUIET
ENDIF
WAIT 2
SYS:Prefs/Locale

